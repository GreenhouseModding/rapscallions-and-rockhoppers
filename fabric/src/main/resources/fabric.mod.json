{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}+${minecraft_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contributors": [
        "${fabric_mod_contributors}"
    ],
    "contact": {
        "homepage": "${modrinth_page}",
        "issues": "https://github.com/${sources}/issues",
        "sources": "https://github.com/${sources}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "house.greenhouse.rapscallionsandrockhoppers.RapscallionsAndRockhoppersFabric"
        ],
        "client": [
          "house.greenhouse.rapscallionsandrockhoppers.client.RapscallionsAndRockhoppersFabricClient"
        ],
        "fabric-datagen": [
          "house.greenhouse.rapscallionsandrockhoppers.datagen.RockhoppersDatagen"
        ]
    },
    "custom": {
        "cardinal-components": [
            "rapscallionsandrockhoppers:boat_data",
            "rapscallionsandrockhoppers:player_data"
        ],
        "modmenu": {
            "links": {
                "modmenu.curseforge": "${curseforge_page}",
                "modmenu.modrinth": "${modrinth_page}",
                "modmenu.github_releases": "https://github.com/${sources}/releases"
            }
        }
    },
    "accessWidener" : "rapscallionsandrockhoppers.accesswidener",
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "${fabric_loader_range}",
        "fabric-api": "*",
        "minecraft": "${fabric_minecraft_version_range}",
        "java": ">=${java_version}",
        "smartbrainlib": ">=1.13"
    }
}
  